     <script>
        document.getElementById('search').addEventListener('input', function() {
            var filter = this.value.toLowerCase();
            var items = document.querySelectorAll('.programsbox, .programsbox1');
            var found = false;

            items.forEach(function(item) {
                var name = item.querySelector('h1').textContent.toLowerCase();
                if (name.indexOf(filter) > -1) {
                    item.style.display = 'flex';
                    found = true;
                } else {
                    item.style.display = 'none';
                }
            });

            // Show or hide the "No results found" message
            var noResults = document.querySelector('.no-results');
            if (found) {
                noResults.style.display = 'none'; // Hide message if results are found
            } else {
                noResults.style.display = 'block'; // Show message if no results
            }
        });
    </script>
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
